{
  "name": "react-pro",
  "version": "0.0.1",
  "description": "A mini management system based on React",
  "keywords": [
    "React",
    "Admin",
    "Vite",
    "Antd"
  ],
  "homepage": "https://github.com/kerwin-ly/react-pro#readme",
  "bugs": {
    "url": "https://github.com/kerwin-ly/react-pro/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kerwin-ly/react-pro.git"
  },
  "license": "ISC",
  "author": "Kerwin",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "preview": "vite preview",
    "build": "tsc && vite build --mode production",
